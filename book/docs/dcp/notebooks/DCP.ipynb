   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "expression curvature = CONVEX\n",
      "expression sign = NONNEGATIVE\n",
      "expression is DCP? True\n"
     ]
    }
   ],
   "source": [
    "import cvxpy as cp\n",
    "\n",
    "x = cp.Variable()\n",
    "y = cp.Variable()\n",
    "expr = cp.quad_over_lin(x - y, 1 - cp.maximum(x, y))\n",
    "print(\"expression curvature =\", expr.curvature)\n",
    "print(\"expression sign =\", expr.sign)\n",
    "print(\"expression is DCP?\", expr.is_dcp())"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
     "name": "#%%\n"
    }
   }
  }
